<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Following</title>
    <link rel="stylesheet" href="/user-following.css">
    <link rel="stylesheet" href="/navbar.css">
    <link rel="stylesheet" href="https://maxst.icons8.com/vue-static/landings/line-awesome/line-awesome/1.3.0/css/line-awesome.min.css">
  </head>
  <body>
  <div id="grid">
    <div class="links-content" id="navbar-grid-size">
      <a href="/logout" class="float-right button-styling"><i class="las la-sign-out-alt icon-size-spacing"></i>Logout</a>
      <a href="/search" class="button-styling"><i class="las la-search icon-size-spacing"></i>Search</a>
      <a href="/profile" class="button-styling"><i class="las la-user icon-size-spacing"></i>Profile</a>
    </div>
  <div id="following-box-header">
    People you follow
  </div>
  {{#each userFollowing}}
  <div id="following-box">
    <a href="/user/{{this}}">{{this}}</a>
  </div>
  {{/each}}
  </div>
  </body>
</html>
